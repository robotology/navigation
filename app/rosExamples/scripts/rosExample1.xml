<application>
<name>ros EXAMPLE1</name>

<dependencies>
</dependencies>

<module>
   <name>yarpdev</name>
   <parameters>--device transformServer --ROS "(enable_ros_publisher 1)" "(enable_ros_subscriber 1)"</parameters>
   <node>console</node>
</module>

<module>
   <name>yarpdev</name>
   <parameters>--device fakeMotionControl --name /robot_2wheels/mobile_base --GENERAL::Joints 2</parameters>
   <node>console</node>
</module>

<module>
   <name>baseControl</name>
   <parameters>--context baseControl_SIM --from robot_2wheels.ini --skip_robot_interface_check</parameters>
   <node>console</node>
</module>

<module>
   <name>yarpdev</name>
   <parameters>--device map2DServer --mapCollectionContext mapsExample</parameters>
   <node>console</node>
</module>

<module>
   <name>roslaunch</name>
   <parameters>~/navigation/app/rosExamples/launch/amcl_map_from_yarp.launch</parameters>
   <node>ros_pc</node>
</module>

<module>
   <name>localizationServer</name>
   <parameters>--context rosExamples --from localizationServer_yarp.ini</parameters>
   <node>console</node>
</module>

<module>
   <name>yarpdev</name>
   <parameters>--device Rangefinder2DWrapper --subdevice fakeLaser --period 10 --name /ikart/laser:o --test use_mapfile --map_file C:\\software\\navigation\\app\\mapsExample\\map_test.map --localization_client</parameters>
   <node>console</node>
</module>

<module>
   <name>yarpdev</name>
   <parameters>--device navigation2DServer --subdevice robotPathPlannerDev --context robotPathPlannerExamples --from robotPathPlanner_robot_2wheels.ini</parameters>
   <node>console</node>
</module>

<module>
   <name>navigationGUI</name>
   <parameters></parameters>
   <node>console</node>
</module>

<module>
   <name>yarpview</name>
   <parameters>--name /plannerView:i --out /mapClick:o</parameters>
   <node>console</node>
</module>

<connection>
  <from>/robotGoto/control:o</from>
  <to>/baseControl/control:i</to>
  <protocol>udp</protocol>
</connection>

<connection>
  <from>/robotPathPlanner/map:o</from>
  <to>/plannerView:i</to>
  <protocol>udp</protocol>
</connection>

<connection>
  <from>/robotPathPlanner/commands:o</from>
  <to>/robotGoto/rpc</to>
  <protocol>tcp</protocol>
</connection>

<connection>
  <from>/robotGoto/status:o</from>
  <to>/robotPathPlanner/navigationStatus:i</to>
  <protocol>tcp</protocol>
</connection>

<connection>
  <from>/mapClick:o</from>
  <to>/robotPathPlanner/yarpviewTarget:i</to>
  <protocol>tcp</protocol>
</connection>

<connection>
  <from>/robotPathPlanner/yarpviewTarget:o</from>
  <to>/robotPathPlanner/rpc</to>
  <protocol>tcp</protocol>
</connection>

<connection>
  <from>/baseControl/odometry:o</from>
  <to>/fakeLaser/location:i</to>
  <protocol>tcp</protocol>
</connection>

</application>
